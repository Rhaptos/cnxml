<!ENTITY % CNXML.proof.content "((%CNXML.name.qname;)?, (%CNXML.rule.qname;|%CNXML.para.qname;|%CNXML.equation.qname;|%CNXML.figure.qname;|%CNXML.list.qname;)+)">
<!ELEMENT %CNXML.proof.qname; %CNXML.proof.content;>
